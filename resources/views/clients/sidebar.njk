<div class="div-toggle"><i class="fa fa-bars"></i></div>
<div class="div-navblock">
  <div class="menu-space">
    <div class="content">
      <div class="sidebar-logo">
        <div class="logo">
          <a href="index2.html">
            RESUME<span class="blue">PRO</span>
          </a>
        </div>
      </div>
      <ul class="div-vnavigation" style="display: none;">
        {% asyncAll m in menu%}
              {% if m.child %}
                <li class="parent {{m.custom_class}}"><a href="#"><i class="fa fa-{{m.fa_icon}}" aria-hidden="true"></i><span>{{lang(m.label)}}</span></a>
                    <ul class="sub-menu" style="">
                    {% asyncAll ch in m.child%}
                      <li>
                        <a href="{{ch.url}}">
                          {% if ch.isnew %}
                            <span class="label label-primary pull-right">New</span>
                          {% endif %}
                          {{lang(ch.label)}}
                        </a>
                      </li>
                    {% endall %}
                    </ul>
                </li>
              {% else %}
                <li class=""><a href="{{m.url}}"><i class="fa fa-{{m.fa_icon}}" aria-hidden="true"></i><span>{{lang(m.label)}}</span></a>
                </li>
              {% endif %}
        {% endall %}
      </ul>

    </div>
  </div>
  <div class="text-right collapse-button" style="padding:7px 9px;">
    <input type="text" class="form-control search" placeholder="Search...">
    <button id="sidebar-collapse" class="btn btn-default" style=""><i style="color:#fff;" class="fa fa-angle-left"></i></button>
  </div>
</div>
      <ul class="div-vnavigation" style="display: none;">
        {% asyncAll m in menu%}
              {% if m.child %}
                <li class="parent {{m.custom_class}}"><a href="#"><i class="fa fa-{{m.fa_icon}}"></i><span>{{lang(m.label)}}</span></a>
                    <ul class="sub-menu" style="">
                    {% asyncAll ch in m.child%}
                      <li>
                        <a href="{{ch.url}}">
                          {% if ch.isnew %}
                            <span class="label label-primary pull-right">New</span>
                          {% endif %}
                          {{lang(ch.label)}}
                        </a>
                      </li>
                    {% endall %}
                    </ul>
                </li>
              {% else %}
                <li class=""><a href="{{m.url}}"><i class="fa fa-{{m.fa_icon}}"></i><span>{{lang(m.label)}}</span></a>
                </li>
              {% endif %}
        {% endall %}
      </ul>

      <div class="side-user">
        <div class="avatar icon-logo">
          <i class="fa fa-hdd-o"></i>
        </div>
        <div class="info">
          <p>40 <b>GB</b> / 100 <b>GB</b></p>
          <div class="progress progress-user">
            <div class="progress-bar" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
              <span class="sr-only">50% Complete (success)</span>
            </div>
          </div>
        </div>
